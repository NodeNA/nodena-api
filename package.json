{
  "name": "nodena-api",
  "version": "1.0.0",
  "description": "An information portal API  and social platform for Node.js  developers in Namibia",
  "main": "app.js",
  "scripts": {
    "migrate": "knex migrate:latest && knex seed:run",
    "start": "export NODE_ENV=staging && nodemon app.js",
    "debug": "export NODE_ENV=staging && node --inspect app.js",
    "production": "export NODE_ENV=production && pm2 start app.js --name='NodeNA'",
    "dev": "export NODE_ENV=development && nodemon",
    "staging": "export NODE_ENV=staging && nodemon",
    "test": "export NODE_ENV=testing && ./node_modules/mocha/bin/mocha test",
    "setup": "node db/init.js",
    "postmerge": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NodeNA/nodena-api.git"
  },
  "keywords": [
    "nodejs",
    "events",
    "neetups",
    "coding",
    "networking",
    "namibia",
    "developers",
    "programmers",
    "react"
  ],
  "author": "Geraldo Kandonga Fillipus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NodeNA/nodena-api/issues"
  },
  "homepage": "https://github.com/NodeNA/nodena-api#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.4.7",
    "bcrypt-nodejs": "^0.0.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dateformat": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-redis-cache": "^1.1.3",
    "express-validator": "^6.12.1",
    "ghreleases": "^3.0.2",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.6.5",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.5.0",
    "passport-github": "^1.1.0",
    "pug": "^3.0.2",
    "redis": "^3.1.2",
    "rss": "^1.2.2",
    "sqlite3": "^5.0.2",
    "twitter": "^1.7.1",
    "type-is": "^1.6.18",
    "unidecode": "^0.1.8",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "mocha": "^9.1.2",
    "simple-prompt": "^0.2.1",
    "standard": "^16.0.3",
    "supertest": "^6.1.6"
  }
}
